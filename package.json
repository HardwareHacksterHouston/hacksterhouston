        "connect-redis": "^3.4.0",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "faker": "^4.1.0",
        "lodash": "^4.17.11",
        "redis": "^2.8.0",
        "yargs": "^12.0.5"
    },
    "babel": {
        "presets": [
            "react"
        ]
    }
